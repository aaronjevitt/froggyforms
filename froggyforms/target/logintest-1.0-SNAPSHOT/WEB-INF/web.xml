        <servlet-class>controllers.AdminController</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>Submission</display-name>
        <servlet-name>Submission</servlet-name>
        <servlet-class>controllers.FormRequestController</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>FroggyPublisher</display-name>
        <servlet-name>FroggyPublisher</servlet-name>
        <servlet-class>controllers.FroggyPublisher</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>PublishedPage</display-name>
        <servlet-name>PublishedPage</servlet-name>
