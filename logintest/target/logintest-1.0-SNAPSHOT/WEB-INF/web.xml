<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <display-name>MongoDBWebapp</display-name>
    <welcome-file-list>
        <welcome-file>login_logout.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <description/>
        <display-name>FroggyHome</display-name>
        <servlet-name>FroggyHome</servlet-name>
        <servlet-class>controllers.FroggyLogin</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>AdminPage</display-name>
        <servlet-name>AdminPage</servlet-name>
        <servlet-class>controllers.AdminController</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>Submission</display-name>
        <servlet-name>Submission</servlet-name>
        <servlet-class>controllers.SubmissionController</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>FroggyPublisher</display-name>
        <servlet-name>FroggyPublisher</servlet-name>
        <servlet-class>controllers.FroggyPublisher</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>PublishedPage</display-name>
        <servlet-name>PublishedPage</servlet-name>
        <servlet-class>controllers.PublishedPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FroggyHome</servlet-name>
        <url-pattern>/FroggyHome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminPage</servlet-name>
        <url-pattern>/AdminPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Submission</servlet-name>
        <url-pattern>/Submission</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FroggyPublisher</servlet-name>
        <url-pattern>/FroggyPublisher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PublishedPage</servlet-name>
        <url-pattern>/PublishedPage</url-pattern>
    </servlet-mapping>
</web-app>
