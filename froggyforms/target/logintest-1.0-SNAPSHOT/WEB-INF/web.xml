        <servlet-name>SubmissionDeletionController</servlet-name>
        <servlet-class>controllers.SubmissionDeletionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FormDeletionController</servlet-name>
        <servlet-class>controllers.FormDeletionController</servlet-class>
    </servlet> 
    <servlet>
        <servlet-name>FormRequestController</servlet-name>
        <servlet-class>controllers.FormRequestController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FroggyHome</servlet-name>
        <url-pattern>/FroggyHome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminPage</servlet-name>
        <url-pattern>/AdminPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Submission</servlet-name>
        <url-pattern>/Submission</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FroggyPublisher</servlet-name>
        <url-pattern>/FroggyPublisher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PublishedPage</servlet-name>
        <url-pattern>/PublishedPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FormRequestController</servlet-name>
        <url-pattern>/f/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmissionController</servlet-name>
        <url-pattern>/f/submit</url-pattern>
    </servlet-mapping>  
    <servlet-mapping>
        <servlet-name>SubViewController</servlet-name>
        <url-pattern>/subview</url-pattern>
    </servlet-mapping>
